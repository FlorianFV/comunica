{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces"],
    "ter-indent": [true, 2],
    "no-empty-interface": false,
    "quotemark": false,
    "no-trailing-whitespace": [true, "ignore-jsdoc"],
    "no-angle-bracket-type-assertion": false,
    "member-ordering": [true, {
      "order": "fields-first"
    }],
    "forin": false
  },
  "rulesDirectory": []
}
